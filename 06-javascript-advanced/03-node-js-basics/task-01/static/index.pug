html
    head
        link(rel='stylesheet', href='css/style.css')
    body
        each val, index in category
            hi=index
            ul
                each url in val
                        li= url
                            a(class="button--delete button" data-id=`${url}`) delete
                            a(class="button--download button" href=`${url}`) download
script(src='js/main.js')


